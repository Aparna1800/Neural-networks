        "#defining the function\n",
        "def string_alternative(string):\n",
        "    o=''\n",
        "#Iterative loop\n",
        "    for i in range(len(string)):\n",
        "        if(i%2==0):\n",
        "            o=o+string[i]\n",
        "    return o\n",
        "if __name__=='__main__':\n",
        "#Print the result for given string\n",
        "    print(string_alternative(\"Good Evening\"))"
      ]
    }
